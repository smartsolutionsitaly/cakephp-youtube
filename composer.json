{
  "name": "smartsolutionsitaly/cakephp-youtube",
  "description": "YouTube plugin for CakePHP",
  "keywords": [
    "cakephp",
    "youtube",
    "client"
  ],
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "google/apiclient": "^2.2"
  },
  "require-dev": {
    "cakephp/cakephp": "^3.6",
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "autoload": {
    "psr-4": {
      "SmartSolutionsItaly\\CakePHP\\YouTube\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmartSolutionsItaly\\CakePHP\\YouTube\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "homepage": "https://github.com/smartsolutionsitaly/cakephp-youtube",
  "authors": [
    {
      "name": "Lucio Benini",
      "email": "dev@smartsolutions.it",
      "homepage": "https://smartsolutions.it",
      "role": "Web Developer"
    }
  ],
  "support": {
    "email": "info@smartsolutions.it",
    "issues": "https://github.com/smartsolutionsitaly/cakephp-youtube/issues",
    "wiki": "https://github.com/smartsolutionsitaly/cakephp-youtube/wiki",
    "source": "https://github.com/smartsolutionsitaly/cakephp-youtube"
  }
}
